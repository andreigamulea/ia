<h2>Ultimele 30 de vizite</h2>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>User ID</th>
      <th>Nume</th>
      <th>User Agent</th>
      <th>Landing Page</th>
      <th>Browser</th>
      <th>OS</th>
      <th>Device Type</th>
      <th>Started At</th>
    </tr>
  </thead>
  <tbody>
    <% @ahoy_visits.each do |visit| %>
      <tr>
        <td><%= visit.user_id %></td>
        <td><%= visit.user.name %></td> <!-- Presupunand ca numele este un atribut al modelului User -->
        <td><%= visit.user_agent %></td>
        <td><%= visit.landing_page %></td>
        <td><%= visit.browser %></td>
        <td><%= visit.os %></td>
        <td><%= visit.device_type %></td>
        <td><%= visit.started_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<h2>Ultimele 70 de evenimente</h2>
<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Id</th>  
      <th>User ID</th>
      <th>Nume</th>
      <th>Name</th>
      <th>Properties</th>
      <th>Time</th>
    </tr>
  </thead>
  <tbody>
    <% @ahoy_events.each do |event| %>
      <tr>
      <td><%= event.id %></td>
        <td><%= event.user_id %></td>
        <td><%= event.user&.name %></td>
        <td><%= event.name %></td>
        <td><%= event.properties %></td>
        <td><%= event.time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
